// @flow

import * as React from 'react';
import { TouchableWithoutFeedback, View } from 'react-native';

import type { ClickableProps } from './Clickable.types';

const nothing = () => {};
const Clickable = ({ children, onPress, onClick, role = 'none' }: ClickableProps) =>
<TouchableWithoutFeedback onPress={onPress || onClick || nothing} accessibilityRole={role}>
    <View>{children}</View>
  </TouchableWithoutFeedback>;


export { Clickable };