// @flow

import { type PressEvent } from 'react-native/Libraries/Types/CoreEventTypes';

import type { onClickEvent } from '../common/common.types';

type Role =
'none' |
'button' |
'link' |
'search' |
'image' |
'keyboardkey' |
'text' |
'adjustable' |
'imagebutton' |
'header' |
'summary';

export type ClickableProps = {|
  +children: React$Node,
  +href?: string,
  +onClick?: (onClickEvent) => void,
  +onPress?: ?(event: PressEvent) => mixed,
  +rel?: string,
  +role?: Role,
  +target?: string,
  +onKeyPress?: (SyntheticKeyboardEvent<HTMLElement>) => void,
  +tabIndex?: number, |};


declare export default React$ComponentType<ClickableProps>;