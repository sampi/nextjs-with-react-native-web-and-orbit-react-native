// @flow strict

const ucCase = require('../../common/utils').ucCase;


type IconListType = {
  [string]: {
    description: string,
    category: string,
    character: string,
    svg: string, ...}, ...};






module.exports = {
  output: 'src/Icon/icons.export.generated.js',
  input: 'static/icons.json',
  generate: function generateAggregatedIconExports(
  iconList: IconListType,
  HEADER: string)
  {
    return `// @flow

${HEADER}

import { icons } from './index';
${Object.keys(iconList).
    map(function (dashCaseIconName) {
      const ucCaseIconName = ucCase(dashCaseIconName);
      return `
const ${ucCaseIconName} = icons.${ucCaseIconName};
${ucCaseIconName}.displayName = '${ucCaseIconName}';
export { ${ucCaseIconName} };`;
    }).
    join('\n')}
`;
  } };