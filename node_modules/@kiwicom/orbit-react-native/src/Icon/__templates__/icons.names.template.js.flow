// @flow strict

const ucCase = require('../../common/utils').ucCase;


type IconListType = {
  [string]: {
    description: string,
    category: string,
    character: string,
    svg: string, ...}, ...};





module.exports = {
  output: 'src/Icon/icons.names.generated.js',
  input: 'static/icons.json',
  generate: function generateIconTypesAndNames(
  iconList: IconListType,
  HEADER: string)
  {
    const icons = {};
    Object.keys(iconList).forEach(function (iconName) {
      icons[iconName] = iconList[iconName].character;
    });

    return `// @flow strict

${HEADER}

export const icons = {
  ${Object.keys(icons).
    map(function (iconName) {
      return `${ucCase(iconName)}: '${icons[iconName]}',`;
    }).
    join('\n  ')}
};
`;
  } };