// @flow

import { createElement } from 'react-native-web';

import type { WebTextLinkContainer } from '../TextLink.types';


export function TextLinkContainer({ asComponent = 'a', tabIndex, href, ...props }: WebTextLinkContainer) {
  return createElement(asComponent, { ...props, href, tabIndex: tabIndex || (!href ? '0' : undefined) });
}

// Display name for Storybook
TextLinkContainer.displayName = 'TextLinkContainer';